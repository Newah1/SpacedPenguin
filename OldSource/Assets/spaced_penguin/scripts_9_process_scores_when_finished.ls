property pScoresNotUpdatedon beginSprite me  pScoresNotUpdated = 1endon enterFrame me  global gHSNetID  if pScoresNotUpdated then    statusList = getStreamStatus(gHSNetID)    if statusList.state = "complete" then      processHSNetString()      pScoresNotUpdated = 0    end if    if not ((netError(gHSNetID) = EMPTY) or (netError(gHSNetID) = "OK")) then      alert("Sorry, there was an error loading" & RETURN & "the highscores.")      pScoresNotUpdated = 0    end if  end ifend