property pSprite, pValue, pState, pRotationVelglobal gBonuson beginSprite me  pState = #notHit  pSprite = sprite(me.spriteNum)  if gBonus = VOID then    gBonus = []  end if  gBonus.add(pSprite.spriteNum)  pRotationVel = 3.0endon collectBonus me  if pState = #notHit then    pRotationVel = 30.0    puppetSound(2, "snd_bonus")    pState = #Hit    pSprite.member = member(pSprite.memberNum + 1)    sendSprite(39, #showBonus, pValue, pSprite.loc)    return pValue  else    return 0  end ifendon resetBonus me  if pState = #Hit then    pRotationVel = 3.0    pState = #notHit    pSprite.member = member(pSprite.memberNum - 1)  end ifendon prepareFrame me  if pRotationVel > 3.0 then    pRotationVel = pRotationVel - 0.10000000000000001  else    pRotation = 3.0  end if  pSprite.rotation = pSprite.rotation + pRotationVelendon endSprite me  gBonus = []endon getPropertyDescriptionList  return [#pValue: [#comment: "Value of this bonus:", #format: #integer, #default: 100, #range: [#min: 0, #max: 10000]]]end